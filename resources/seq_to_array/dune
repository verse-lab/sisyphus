(library (name seq_to_array)
  (modules common)
)

(coq.theory
 (name Proofs)
 (modules
   Common_ml
   Verify_seq_to_array_utils
   Seq_to_array_old_ml Verify_seq_to_array_old
   Seq_to_array_new_ml Verify_seq_to_array_new

 )
)


(rule
 (target Common_ml.v)
 (deps common.ml)
 (action (run cfmlc -o ./Common_ml.v common.ml)))

(rule
 (target Seq_to_array_old_ml.v)
 (deps seq_to_array_old.ml Common_ml.v)
 (action (run cfmlc -o ./Seq_to_array_old_ml.v seq_to_array_old.ml)))

(rule
 (target Seq_to_array_new_ml.v)
 (deps seq_to_array_new.ml Common_ml.v)
 (action (run cfmlc -o ./Seq_to_array_new_ml.v seq_to_array_new.ml)))
